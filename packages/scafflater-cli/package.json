{
  "name": "scafflater-cli",
  "description": "The Scafflater Cli",
  "author": "chicoribas @chicoribas",
  "bin": "./bin/run",
  "bugs": "https://github.com/chicoribas/scafflater/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "chalk": "^4.1.1",
    "inquirer": "^8.1.1",
    "ora": "^5.4.1",
    "scafflater": "*"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/commands",
    "/index.js",
    "/util"
  ],
  "homepage": "https://github.com/chicoribas/scafflater",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "oclif": {
    "commands": "./commands",
    "bin": "scafflater-cli",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "partial": {
        "description": "Partial template operations"
      },
      "partial:add": {
        "description": "Adds a partial to the output folder"
      },
      "partial:list": {
        "description": "List available partials in template"
      }
    }
  },
  "repository": "chicoribas/scafflater",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "version": "oclif-dev readme && git add README.md"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0"
  }
}
