{
  "name": "scafflater-cli",
  "description": "The Scafflater Cli",
  "version": "0.0.1",
  "author": "chicoribas @chicoribas",
  "bin": "./bin/run",
  "bugs": "https://github.com/chicoribas/scafflater/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "chalk": "^4.1.1",
    "fs-util": "workspace:*",
    "inquirer": "^8.1.0",
    "logger": "workspace:*",
    "ora": "^5.4.1",
    "scafflater": "workspace:*",
    "template-cache": "workspace:*",
    "template-manager": "workspace:*",
    "template-source": "workspace:*"
  },
  "engines": {
    "node": ">=13.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/commands",
    "/index.js"
  ],
  "homepage": "https://github.com/chicoribas/scafflater",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "index.js",
  "oclif": {
    "commands": "./commands",
    "bin": "scf-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "chicoribas/scafflater",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0"
  }
}
