{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest/globals": true
  },
  "extends": [
    "standard",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:jest/recommended",
    "plugin:node/recommended",
    "plugin:prettier/recommended",
    "plugin:promise/recommended"
  ],
  "plugins": [
    "jest",
    "prettier"
  ],
  //"parser": "@babel/eslint-parser",
  // "ignorePatterns": [
  //   "lcov-report"
  // ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "space-before-function-parent": 0
  }
}
